{
  "name": "csam",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "test:watch": "vitest --coverage",
    "lint": "eslint_d --quiet `npm run --silent list-files`",
    "lint:fix": "eslint_d --fix `npm run --silent list-files`",
    "lint:loud": "eslint_d `npm run --silent list-files`",
    "eslint": "eslint",
    "list-files": "git ls-files '*.js' '*.cjs' '*.mjs' '*.jsx', '*.ts', '*.tsx'",
    "cypress": "cypress open",
    "cypress:component": "cypress run --component",
    "check-types": "tsc",
    "find-dead-code": "knip --production --strict --exclude types",
    "cover:unit": "nyc --cache false --no-clean --silent npm run test",
    "cover:component": "nyc --cache false --silent npm run cypress:component",
    "cover:report": "nyc report --reporter=html-spa --reporter=text",
    "cover:check": "npm run cover:report --  --check-coverage",
    "cover": "npm run cover:component && npm run cover:unit && npm run cover:report"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.37.0",
    "@azure/msal-react": "^1.5.7",
